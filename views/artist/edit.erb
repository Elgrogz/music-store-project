<h1>Edit Artist</h1>

<form action="/artists/<%= @artist.id %>" method="post">
  <p>
    <label>
      Artist name:
      <input type="text" name="name" value="<%= @artist.name %>"/>
    </label>
  </p>

  
  <p>
    <label>
      Image Url (optional):
      <input type="text" name="artist_url"/>
    </label>
  </p>

  <input type="submit" value="Edit Artist"/>

</form>